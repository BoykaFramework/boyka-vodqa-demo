<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Boyka Framework Sample Web Suite" verbose="2">
    <test name="Sample Test Web" enabled="false">
        <parameter name="platformType" value="WEB"/>
        <parameter name="configKey" value="test_sauce_web"/>
        <classes>
            <class name="com.boyka.demo.ui.sauce.SauceDemoTest"/>
        </classes>
    </test>
    <test name="Sample Test Android" enabled="false">
        <parameter name="platformType" value="ANDROID"/>
        <parameter name="configKey" value="test_sauce_android"/>
        <classes>
            <class name="com.boyka.demo.ui.sauce.SauceDemoTest"/>
        </classes>
    </test>
    <test name="Sample Test iOS">
        <parameter name="platformType" value="IOS"/>
        <parameter name="configKey" value="test_sauce_ios"/>
        <classes>
            <class name="com.boyka.demo.ui.sauce.SauceDemoTest"/>
        </classes>
    </test>
</suite>
